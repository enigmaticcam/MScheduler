@using MScheduler_Web.Models;
@model ViewControlListAsLinks

@if (Model.LinksDisplayName.Count > 0) { 
    for (int i = 0; i < Model.LinksDisplayName.Count; i++) {
         @:@Html.ActionLink(Model.LinksDisplayName[i], Model.LinksActionName[i], Model.LinksControlName[i], Model.LinksModel[i], new { @class = "btn btn-default" })<br />         
    }
} else {
    @Html.DisplayTextFor(m => m.TextIfThereAreNoLinks);   
}