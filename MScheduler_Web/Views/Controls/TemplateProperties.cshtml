@using MScheduler_BusTier.Concrete
@model EditTemplateView.Baton

@using (Html.BeginForm("EditTemplate", "EditTemplate", FormMethod.Post)) {
    @Html.HiddenFor(m => m.Id)
    <input type="submit" value="Update" class="btn btn-primary btn-sm" />
    <br />
    <br />
    <div class="container-fluid">
        <div class="row">
            <div class="form-group">
                <label>Id</label>
                <div>@Model.Id</div>
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                <label>Description</label>
                <div>@Html.TextBoxFor(m => m.Description, new { @class = "form-control" })</div>
            </div>
        </div>
    </div>
}